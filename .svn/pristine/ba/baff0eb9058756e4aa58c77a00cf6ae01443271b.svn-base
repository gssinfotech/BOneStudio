<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   android:background="@drawable/app_background">
	<TextView
		android:id="@+id/mydevices_central_heading"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginBottom="10dp"
		android:padding="5dp"
		android:text="@string/device_central_heading"
		android:gravity="center"
		android:textColor="@color/white"
		android:textStyle="bold"
		android:textSize="17sp"
		android:layout_marginTop="10dp"/>
	<ImageButton
		android:id="@+id/acc_creation_backbtn"
		android:layout_width="30dp"
		android:layout_height="wrap_content"
		android:background="@drawable/back_arrow_btn"
		android:layout_alignBottom="@+id/mydevices_central_heading"
		android:layout_alignLeft="@+id/mydevices_central_heading"
		android:layout_alignStart="@+id/mydevices_central_heading"
		android:layout_alignTop="@+id/mydevices_central_heading"
		android:contentDescription="@string/app_name"
		/>
      <Button 
	    android:id="@+id/device_central_actionbarhome_button_id"
	    android:layout_width="30dp"
	    android:layout_height="30dp"
	    android:layout_alignParentRight="true"
	    android:layout_alignParentEnd="true"
	    android:layout_marginRight="20dp"
	     android:layout_marginEnd="20dp"
	    android:layout_marginBottom="10dp"
	    android:layout_marginTop="5dp"
	    android:background="@drawable/home"
	    android:onClick="onHomeButtonClicked"/>
	 <ScrollView 
  	     android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@+id/mydevices_central_heading"
  	 >
  	     
	<RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   	>
  	
      <Button 
        android:id="@+id/mydevices_central_lights_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mydevices_central_lights_str"
        android:textColor="@android:color/white"
        android:background="@color/btn_background"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="20dp"
        android:gravity="start|center_vertical"
		android:paddingTop="20dp"
		android:paddingBottom="20dp"
        android:paddingStart="45dp"
        android:paddingEnd="45dp"
        android:onClick="onLightsButtonClicked"/>
	      <ImageView
	         
	 	    android:layout_width="40dp"
	 	    android:layout_height="wrap_content"
	 	    android:layout_alignBottom="@+id/mydevices_central_lights_btn"
	 	    android:layout_alignLeft="@+id/mydevices_central_lights_btn"
	 	    android:layout_alignStart="@+id/mydevices_central_lights_btn"
	 	    android:layout_alignTop="@+id/mydevices_central_lights_btn"
	 	    android:src="@drawable/mydevices_lights_icon"
	 	    android:padding="5dp"
	 	    android:contentDescription="@string/app_name"/>
	    
	     <ImageView
	 	    android:layout_width="wrap_content"
	 	    android:layout_height="wrap_content"
	 	    android:layout_alignBottom="@+id/mydevices_central_lights_btn"
	 	    android:layout_alignRight="@+id/mydevices_central_lights_btn"
	 	    android:layout_alignEnd="@+id/mydevices_central_lights_btn"
	 	    android:layout_alignTop="@+id/mydevices_central_lights_btn"
	 	    android:src="@drawable/button_right"
	 	    android:layout_marginRight="5dp"
	 	    android:layout_marginEnd="5dp"
	 	    android:padding="10dp"
	 	    android:contentDescription="@string/app_name"/>
	  		  
    <Button 
        android:id="@+id/mydevices_central_thermostat_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mydevices_central_thermostat_str"
        android:textColor="@android:color/white"
        android:background="@color/btn_background"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="20dp"
        android:gravity="start|center_vertical"
        android:paddingStart="45dp"
        android:paddingEnd="45dp"
        android:layout_below="@id/mydevices_central_lights_btn"
        android:onClick="onThermostatButtonClicked"
		android:paddingTop="20dp"
		android:paddingBottom="20dp"
		/>

	      <ImageView
	          android:layout_width="40dp"
	          android:layout_height="wrap_content"
	          android:layout_alignBottom="@+id/mydevices_central_thermostat_btn"
	          android:layout_alignLeft="@+id/mydevices_central_thermostat_btn"
	          android:layout_alignStart="@+id/mydevices_central_thermostat_btn"
	          android:layout_alignTop="@+id/mydevices_central_thermostat_btn"
	          android:contentDescription="@string/app_name"
	          android:padding="5dp"
	          android:src="@drawable/mydevices_thermostat_icon"
			  />
	    
	     <ImageView
	 	    android:layout_width="wrap_content"
	 	    android:layout_height="wrap_content"
	 	    android:layout_alignBottom="@+id/mydevices_central_thermostat_btn"
	 	    android:layout_alignRight="@+id/mydevices_central_thermostat_btn"
	 	    android:layout_alignEnd="@+id/mydevices_central_thermostat_btn"
	 	    android:layout_alignTop="@+id/mydevices_central_thermostat_btn"
	 	    android:src="@drawable/button_right"
	 	    android:layout_marginRight="5dp"
	 	    android:layout_marginEnd="5dp"
	 	    android:padding="10dp"
	 	    android:contentDescription="@string/app_name"/>
	     
	      <Button 
        android:id="@+id/mydevices_central_switches_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mydevices_central_switches_str"
        android:textColor="@android:color/white"
        android:background="@color/btn_background"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="20dp"
        android:gravity="start|center_vertical"
        android:paddingStart="45dp"
        android:paddingEnd="45dp"
        android:layout_below="@id/mydevices_central_thermostat_btn"
        android:onClick="onSwitchesButtonClicked"
			  android:paddingTop="20dp"
			  android:paddingBottom="20dp"
			  />

	      <ImageView
	          android:layout_width="40dp"
	          android:layout_height="wrap_content"
	          android:layout_alignBottom="@+id/mydevices_central_switches_btn"
	          android:layout_alignLeft="@+id/mydevices_central_switches_btn"
	          android:layout_alignStart="@+id/mydevices_central_switches_btn"
	          android:layout_alignTop="@+id/mydevices_central_switches_btn"
	          android:contentDescription="@string/app_name"
	          android:padding="5dp"
	          android:src="@drawable/mydevices_switches_icon" />
	    
	     <ImageView
	 	    android:layout_width="wrap_content"
	 	    android:layout_height="wrap_content"
	 	    android:layout_alignBottom="@+id/mydevices_central_switches_btn"
	 	    android:layout_alignRight="@+id/mydevices_central_switches_btn"
	 	    android:layout_alignEnd="@+id/mydevices_central_switches_btn"
	 	    android:layout_alignTop="@+id/mydevices_central_switches_btn"
	 	    android:src="@drawable/button_right"
	 	    android:layout_marginRight="5dp"
	 	    android:layout_marginEnd="5dp"
	 	    android:padding="10dp"
	 	    android:contentDescription="@string/app_name"/>
	     
	        <Button 
        android:id="@+id/mydevices_central_tracker_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mydevices_central_tracker_str"
        android:textColor="@android:color/white"
        android:background="@color/btn_background"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="20dp"
        android:gravity="start|center_vertical"
        android:paddingStart="45dp"
        android:paddingEnd="45dp"
        android:layout_below="@id/mydevices_central_switches_btn"
        android:onClick="onTrackerButtonClicked"
				android:paddingTop="20dp"
				android:paddingBottom="20dp"
				/>

	      <ImageView
	          android:layout_width="40dp"
	          android:layout_height="wrap_content"
	          android:layout_alignBottom="@+id/mydevices_central_tracker_btn"
	          android:layout_alignLeft="@+id/mydevices_central_tracker_btn"
	          android:layout_alignStart="@+id/mydevices_central_tracker_btn"
	          android:layout_alignTop="@+id/mydevices_central_tracker_btn"
	          android:contentDescription="@string/app_name"
	          android:padding="5dp"
	          android:src="@drawable/mydevices_tracker_icon" />
	    
	     <ImageView
	 	    android:layout_width="wrap_content"
	 	    android:layout_height="wrap_content"
	 	    android:layout_alignBottom="@+id/mydevices_central_tracker_btn"
	 	    android:layout_alignRight="@+id/mydevices_central_tracker_btn"
	 	    android:layout_alignEnd="@+id/mydevices_central_tracker_btn"
	 	    android:layout_alignTop="@+id/mydevices_central_tracker_btn"
	 	    android:src="@drawable/button_right"
	 	    android:layout_marginRight="5dp"
	 	    android:layout_marginEnd="5dp"
	 	    android:padding="10dp"
	 	    android:contentDescription="@string/app_name"/>
	         <Button 
        android:id="@+id/mydevices_central_sockets_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mydevices_central_sockets_str"
        android:textColor="@android:color/white"
        android:background="@color/btn_background"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="20dp"
        android:gravity="start|center_vertical"
        android:paddingStart="45dp"
        android:paddingEnd="45dp"
        android:layout_below="@id/mydevices_central_tracker_btn"
        android:onClick="onSocketsButtonClicked"
				 android:paddingTop="20dp"
				 android:paddingBottom="20dp"
				 />

	      <ImageView
	          android:layout_width="40dp"
	          android:layout_height="wrap_content"
	          android:layout_alignBottom="@+id/mydevices_central_sockets_btn"
	          android:layout_alignLeft="@+id/mydevices_central_sockets_btn"
	          android:layout_alignStart="@+id/mydevices_central_sockets_btn"
	          android:layout_alignTop="@+id/mydevices_central_sockets_btn"
	          android:contentDescription="@string/app_name"
	          android:padding="5dp"
	          android:src="@drawable/mydevices_sockets_icon" />
	    
	     <ImageView
	 	    android:layout_width="wrap_content"
	 	    android:layout_height="wrap_content"
	 	    android:layout_alignBottom="@+id/mydevices_central_sockets_btn"
	 	    android:layout_alignRight="@+id/mydevices_central_sockets_btn"
	 	    android:layout_alignEnd="@+id/mydevices_central_sockets_btn"
	 	    android:layout_alignTop="@+id/mydevices_central_sockets_btn"
	 	    android:src="@drawable/button_right"
	 	    android:layout_marginRight="5dp"
	 	    android:layout_marginEnd="5dp"
	 	    android:padding="10dp"
	 	    android:contentDescription="@string/app_name"/>

	     </RelativeLayout>
	     </ScrollView>
</RelativeLayout>
